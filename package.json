{
  "name": "pethreon",
  "private": true,
  "version": "0.1.0",
  "packageManager": "yarn@3.1.1",
  "license": "MIT",
  "homepage": "https://chris56974.github.io/Pethreon/",
  "repository": "github:Chris56974/Pethreon",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "yarn workspace backend run typechain",
    "compile": "yarn workspace backend run compile",
    "test": "yarn workspaces foreach -pi run test",
    "dev": "yarn workspaces foreach -pi run dev",
    "backend:dev": "yarn workspace backend run dev",
    "frontend:dev": "yarn workspace frontend run dev",
    "frontend:build": "yarn workspace frontend run build",
    "frontend:deploy": "yarn workspace frontend run deploy",
    "typechain": "yarn workspace backend run typechain"
  }
}
